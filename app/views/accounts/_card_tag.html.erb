<div class="card mt-4 mb-4">
  <div class="list-group list-group-flush mt-1">
    <h4 class="list-group-item d-flex justify-content-between align-items-center">
      <%= render 'conditions/tag_head' %>
    </h4>
  </div>
    <div class="card-body">
      <ul class="list-group">
        <% if @tags.count != 0 %>
          <%= render 'view_tag' %>
        <% else %>
          None
        <% end  %>
      </ul>
    </div>
</div>
